{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "_comment1": "// task to convert icons to components",
    "icons": "npx @svgr/cli --out-dir app/icons -- ./icons",
    "_comment2": "// watch task",
    "icons:watch": "npm-watch icons",
    "_comment3": "// compile once and start watching for changes",
    "dev:svg": "run-s icons icons:watch",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:*\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "_comment4": "// npm-watch configuration",
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "dependencies": {
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@types/recharts": "^1.8.23",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-select": "^5.4.0",
    "recharts": "2.1.12",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@iconify-icon/react": "^1.0.0-beta.3",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@svgr/cli": "^6.3.1",
    "@svgr/plugin-jsx": "^6.3.1",
    "@svgr/plugin-prettier": "^6.3.1",
    "@svgr/plugin-svgo": "^6.3.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
